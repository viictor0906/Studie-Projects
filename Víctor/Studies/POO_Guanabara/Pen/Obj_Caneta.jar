package Studies.POO_Guanabara.Pen;

public class Obj_Caneta {
    public static void main(String[] args) {
        Caneta c1 = new Caneta(); //cria um novo objeto;
        c1.cor = "AZUL"; //referência a atributo;
        c1.modelo = "BIC";
        c1.valor_carga(); //deixa executar um atributo protegido pois a mesma está dentro de uma classe(public static void main) cujo está utilizando uma classe caneta(Caneta c1 = new Caneta());
        c1.destampar(); //referência a método(pois tem parênteses no fim);

        c1.status();
        c1.rabiscar(); //não está dentro de uma classe caneta, logo não pode ser usado nela.
    }
}
